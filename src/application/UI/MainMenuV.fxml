<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" prefHeight="300.0" prefWidth="275.0" style="-fx-background-color: white;" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox alignment="CENTER" prefHeight="300.0" prefWidth="200.0" AnchorPane.bottomAnchor="8.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="8.0">
			<children>
				<HBox alignment="CENTER" maxWidth="50.0">
					<children>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Teko;" text="CALL">
							<font>
								<Font size="37.5" />
							</font>
						</Text>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" text=" ">
							<font>
								<Font name="Segoe UI" size="3.0" />
							</font>
						</Text>
						<Text stroke="BLACK" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Teko;" text="OF">
							<font>
								<Font size="19.0" />
							</font>
							<HBox.margin>
								<Insets top="-3" />
							</HBox.margin>
						</Text>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" text=" ">
							<font>
								<Font name="Segoe UI" size="3.0" />
							</font>
						</Text>
						<Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Teko;" text="MATHE">
							<font>
								<Font size="37.5" />
							</font>
						</Text>
					</children>
				</HBox>
				<Text strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Syncopate;" text="RECHENMEISTER">
					<font>
						<Font size="16.5"/>
					</font>
					<VBox.margin>
						<Insets top="-15.0" />
					</VBox.margin>
				</Text>
				<Button mnemonicParsing="false" onAction="#trainingBtn_onAction" prefWidth="190.0" stylesheets="@Buttons.css" text="Training">
					<VBox.margin>
						<Insets top="17.0" />
					</VBox.margin>
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Button mnemonicParsing="false" onAction="#duelBtn_onAction" prefWidth="190.0" stylesheets="@Buttons.css" text="Duell">
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
					<VBox.margin>
						<Insets top="4.0" />
					</VBox.margin>
				</Button>
				<Button mnemonicParsing="false" onAction="#competitionBtn_onAction" prefWidth="190.0" stylesheets="@Buttons.css" text="Wettkampf">
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
					<VBox.margin>
						<Insets top="4.0" />
					</VBox.margin>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<Button mnemonicParsing="false" onAction="#settingsBtn_onAction" prefWidth="190.0" stylesheets="@Buttons.css" text="Einstellungen">
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
					<VBox.margin>
						<Insets top="12.0" />
					</VBox.margin>
				</Button>
				<TextField fx:id="usernameTF" alignment="CENTER" maxWidth="190.0" minHeight="40.0" prefWidth="190.0" promptText="Benutzername">
					<font>
						<Font name="Segoe UI" size="16.0" />
					</font>
					<VBox.margin>
						<Insets top="12.0" />
					</VBox.margin>
				</TextField>
						<Hyperlink alignment="TOP_CENTER" onAction="#creditsHyperlink_onAction" stylesheets="@Hyperlink.css" text="NvKG-Inf-S-2019" textFill="#000000e5">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Hyperlink>
			</children>
		</VBox>
	</children>
	<padding>
		<Insets left="10.0" right="10.0" />
	</padding>
</AnchorPane>
